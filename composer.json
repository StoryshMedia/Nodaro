{
    "name": "nodaro/core",
    "type": "library",
    "version": "0.1.0",
    "license": "MIT",
    "description": "Nodaro core package",
    "keywords": [
        "framework",
        "nodaro",
        "business",
        "hub"
    ],
    "homepage": "https://nodaro.de",
    "repositories": [
        {
            "type": "path",
            "url": "./bundle/Smug/SystemBundle"
        },
        {
            "type": "path",
            "url": "./bundle/Smug/SearchBundle"
        },
        {
            "type": "path",
            "url": "./bundle/Smug/FrontendBundle"
        },
        {
            "type": "path",
            "url": "./bundle/Smug/AdministrationBundle"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-ftp": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/core": "^v2.6.4",
        "babdev/pagerfanta-bundle": "^3.6",
        "composer/package-versions-deprecated": "1.11.99.2",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "2.11.*",
        "doctrine/orm": "^2.4.5",
        "easyrdf/easyrdf": "^1.1",
        "ezyang/htmlpurifier": "^4.17",
        "guzzlehttp/guzzle": "^7.0",
        "intervention/image": "^2.7",
        "jms/serializer-bundle": "5.4.*",
        "knplabs/knp-paginator-bundle": "^5.9",
        "lcobucci/jwt": "^5.2",
        "lexik/jwt-authentication-bundle": "2.20.*",
        "mailersend/mailersend": "^0.8.0",
        "markrogoyski/math-php": "^0.52.0",
        "nelmio/cors-bundle": "^1.5 || ^2.0",
        "notfloran/mjml-bundle": "3.8.*",
        "nyholm/psr7": "^1.5",
        "pagerfanta/doctrine-orm-adapter": "^3.6",
        "patreon/patreon": "^1.0",
        "php-http/guzzle7-adapter": "^1.0",
        "phpdocumentor/reflection-docblock": "^3.0 || ^4.0 || ^5.0",
        "psr/container": "^2.0",
        "ramsey/uuid-doctrine": "^2.0",
        "smug/administration-bundle": "^0.1",
        "smug/search-bundle": "^0.1",
        "smug/frontend-bundle": "^0.1",
        "smug/system-bundle": "^0.1",
        "symfony/asset": "6.4.*",
        "symfony/browser-kit": "6.4.*",
        "symfony/cache": "6.4.*",
        "symfony/config": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/dependency-injection": "6.4.*",
        "symfony/doctrine-bridge": "6.0.*",
        "symfony/dom-crawler": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/error-handler": "6.4.*",
        "symfony/event-dispatcher": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/filesystem": "6.4.*",
        "symfony/finder": "6.4.*",
        "symfony/flex": "^2.4",
        "symfony/framework-bundle": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/http-foundation": "6.4.*",
        "symfony/http-kernel": "6.4.*",
        "symfony/mailer": "^6.4",
        "symfony/mime": "6.4.*",
        "symfony/monolog-bridge": "^6.0",
        "symfony/monolog-bundle": "^3.10",
        "symfony/polyfill-iconv": "^1.28",
        "symfony/process": "6.4.*",
        "symfony/property-access": "6.4.*",
        "symfony/property-info": "6.4.*",
        "symfony/routing": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/security-core": "6.4.*",
        "symfony/security-csrf": "6.4.*",
        "symfony/security-http": "6.4.*",
        "symfony/serializer": "6.4.*",
        "symfony/stopwatch": "6.4.*",
        "symfony/string": "6.4.*",
        "symfony/translation": "6.4.*",
        "symfony/twig-bridge": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/validator": "6.4.*",
        "symfony/var-dumper": "6.4.*",
        "symfony/web-link": "6.4.*",
        "symfony/webpack-encore-bundle": "^2.1",
        "symfony/yaml": "6.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "Smug\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smug\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*",
            "docker": false
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.3",
        "phpmd/phpmd": "^2.9",
        "phpunit/phpunit": "^9",
        "symfony/maker-bundle": "^1.9",
        "symfony/phpunit-bridge": "^7.0"
    }
}
